<% include skill_nav.ejs %>
<div class="personal skill">
    <div class="resource">
        <!--信息展示-->
        <div class="show-info sy-hidden">
            <div class="am-u-lg-12">
            <span class="editLabel edit sy-float-r">
                <i class="sy-personal-icon sy-personal-icon-edit"></i>编辑
            </span>
            </div>
            <div class="am-u-lg-12 labels">
                <div>DTP排版经验：</div>
                <div>
                    <span>后端获取</span>
                </div>
            </div>
            <div class="am-u-lg-12 labels">
                <div>擅长软件：</div>
                <div>
                    <span>后端获取</span>
                </div>
            </div>
            <div class="am-u-lg-12 labels">
                <div>任务标签：</div>
                <div>
                    <span>后端获取</span>
                </div>
            </div>
        </div>
        <!--提交、修改信息表单-->
        <form class="add-form" action="#">
            <div class="am-u-lg-12">DTP排版经验(单选)：</div>
            <div class="am-u-lg-12 labels single" prop="DTP排版经验"> <!--选中添加class=‘selected’-->
                <span>1年以下</span>
                <span>1~2年</span>
                <span>2~3年</span>
                <span>3年以上</span>
            </div>
            <div class="am-u-lg-12">擅长软件(可多选)：</div>
            <div class="am-u-lg-12 labels multiple" prop="擅长软件">
                <span>Word</span>
                <span>Excel</span>
                <span>PowerPoint</span>
                <span>Photoshop</span>
                <span>FrameMaker</span>
                <span>AutoCAD</span>
                <span>PageMaker</span>
                <span>Illustrator</span>
                <span>QuarkXpress</span>
                <span>Pm</span>
            </div>
            <div class="am-u-lg-12">任务标签(可多项)：</div>
            <div class="am-u-lg-12 labels multiple" prop="任务标签">
                <span>后端获取</span>
            </div>
            <div class="am-u-lg-12 sy-btns sy-font-md">
                <button class="btn btn-default sy-btn sy-btn-default sy-btn-md cancelBtn">取 消</button>
                <button class="btn btn-default sy-btn sy-btn-green sy-btn-md confirmBtn">提 交</button>
            </div>
        </form>
    </div>
</div>